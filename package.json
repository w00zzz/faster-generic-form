  "type": "module",
  "description": "A generic form component library built with React, Formik and Material UI",
  "author": "w00zzz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/w00zzz/faster-generic-form.git"
  },
  "keywords": [
    "react",
    "formik",
    "material-ui",
    "form",
    "generic-form"
  ],
  "bugs": {
    "url": "https://github.com/w00zzz/faster-generic-form/issues"
  },
  "homepage": "https://github.com/w00zzz/faster-generic-form#readme",
  "main": "./dist/generic-form.cjs.js",
  "module": "./dist/generic-form.es.js",
