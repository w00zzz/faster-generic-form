    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/styled-engine": "^7.3.2",
    "@mui/system": "^7.3.2",
    "axios": "^1.12.2",
